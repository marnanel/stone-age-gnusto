	option arch "z5"
	option output "verify.z5"
	
$main:	print	"*NAME verify\n"
	verify	verok		; is it working?
	print	"*FAIL Verification failed.\n" ; no
	ret	255
verok:	print	"*SUCC Verification succeeded.\n" ; yes
	ret	0
	endproc

